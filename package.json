{
  "name": "@rikthepixel/fuzzystring",
  "version": "1.1.1",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "format": "prettier --write .",
    "build": "tsup",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "lib",
    "package.json",
    "README.md"
  ],
  "sideEffects": false,
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js",
      "types": "./lib//index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "prettier": "^3.3.0",
    "ts-jest": "^29.1.4",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5"
  }
}
